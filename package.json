{
  "name": "workerflow-engine",
  "version": "1.0.0",
  "description": "My cool TypeScript app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.18",
    "pino": "^6.11.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/pino": "^6.3.6",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "author": "Tosone <i@tosone.cn> (https://github.com/tosone)",
  "license": "MIT"
}
